{
  "name": "victorstack-ai/drupal-service-collector-demo",
  "description": "Demo Drupal module for service collectors.",
  "type": "drupal-module",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
    "drupal/coder": "^8.3",
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\service_collector_demo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Drupal\\service_collector_demo\\Tests\\Unit\\": "tests/Unit/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
